{
  "name": "unam-ar/jornada",
  "description": "Sistema de Jornadas de la UNaM",

  "repositories": [{
   "type": "composer",
   "url": "https://satis.siu.edu.ar"
  }
  ],
  
  "config": {
    "bin-dir": "bin"
  },

  "require": {
    "php": ">=5.6.0|<7.1.0",
    "siu-toba/framework": "^2.7.3",
    "siu/instalador": "1.0.9",
    "robmorgan/phinx": "^0.8.1",
    "siu/toba-db": "^1.0.3"
  },


  "require-dev": {
    "phpunit/phpunit": "^4.5.0",
    "friendsofphp/php-cs-fixer": "^1.12.0"
  },
  "autoload": {
    "psr-4": { "": "src" }
  },

  "minimum-stability": "dev",
  "prefer-stable": true
}
